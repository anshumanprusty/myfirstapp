<h1>Students#create</h1>
<p>Find me in app/views/students/create.html.erb</p>

<h2 align="center">Student Registration Form</h2>
<form name="frm1" action="show" method="post">
<table border="1" width="35%" cellpadding="5" cellspacing="0" align="center">
	<tr>
		<td><strong><font color="red" size="-1">*</font>&nbsp;Name</strong></td>
		<td><input type="text" id="txtBox1" name="name"></td>
	</tr>
	<tr>
		<td><strong><font color="red" size="-1">*</font>&nbsp;Email</strong></td>
		<td><input type="text" id="txtBox2" name="email"></td>
	</tr>
	<tr>
		<td><strong>Age</strong></td>
		<td>
			<select id="secBox1" name="age">
				<script>
				for (var i=10; i<31; i++){
				document.write("<option value="+ i + ">" + i +"</option>");
				}
				</script>
			</select>
		</td>
	</tr>
	<tr>
		<td><strong>Gender</strong></td>
		<td>
			<select id="secBox2" name="gender">
				<option value="M">Male</option>
				<option value="F">Female</option>				
			</select>
		</td>
	</tr>

	<tr>
		<td colspan="2" align="center"><input type="submit" id="btn1" name="btnSubmit" value="Submit" onclick="return validate()"></td>
	</tr>
</table>
